// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

model Blog {
  id        String     @id @default(auto()) @map("_id") @db.ObjectId
  href      String
  date      DateTime
  likes     Int
  en        Json
  fi        Json
}

model Project {
  id        String     @id @default(auto()) @map("_id") @db.ObjectId
  video     String
  gitHub    String
  en        Json
  fi        Json
}